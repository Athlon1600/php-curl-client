{
  "name": "athlon1600/php-curl-client",
  "description": "Simple PHP cURL Client",
  "keywords": ["php curl client"],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Curl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Curl\\Tests\\": "tests"
    }
  }
}